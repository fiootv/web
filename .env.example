# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-or-anon-key

# Resend SMTP (for contact form & checkout notifications)
# Get your API key from https://resend.com/api-keys
RESEND_HOST=smtp.resend.com
RESEND_PORT=465
RESEND_USER=resend
RESEND_PASSWORD=your_resend_api_key
RESEND_FROM_EMAIL=onboarding@resend.dev
# Comma-separated for multiple admins: admin1@example.com,admin2@example.com
RESEND_ADMIN_EMAIL=your-admin@example.com
